export class Notification {

  constructor(
    public id: number,
    public type: NotificationType,
    public message: string,
    public timeout: number,
    public bgColor: string,
    public textColor: string,
  ) { }

}

export enum NotificationType {
  success = 0,
  warning = 1,
  error = 2,
  info = 3,
  custom = 4
}

